<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
               
     <context:annotation-config/>
     
     <bean name="mycompany" class="com.springcore.annotations.Company">
        <property name="name" value="Capgemini" />
        <property name="location" value="India" />
    </bean>
 
    <bean name="myemp" class="com.springcore.annotations.Employee">
        <property name="id" value="1000" />
        <property name="name" value="Dinesh Sharma" />
    </bean>
  </beans>